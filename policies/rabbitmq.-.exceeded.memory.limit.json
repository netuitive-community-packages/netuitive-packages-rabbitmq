{
  "policy": {
    "actions": [
      {
        "category": 3,
        "type": "event"
      }
    ],
    "conditions": [
      {
        "analytic": "actual",
        "metric": "rabbitmq.health.mem_used",
        "metricThresholdLevel": "rabbitmq.health.mem_limit",
        "operator": ">"
      }
    ],
    "deleted": false,
    "description": "Memory utilization has exceeded the configured memory limit.",
    "duration": 300,
    "enabled": true,
    "name": "RabbitMQ - Exceeded Memory Limit",
    "scope": {
      "elementType": "SERVER"
    }
  }
}